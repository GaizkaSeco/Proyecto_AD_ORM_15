<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.ProyectosEntity" table="proyectos" schema="proyecto_ad_final">
        <id name="codproye">
            <column name="codproye" sql-type="varchar(6)" length="6"/>
        </id>
        <property name="nombre">
            <column name="nombre" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="ciudad">
            <column name="ciudad" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="estado">
            <column name="estado" sql-type="varchar(4)" length="4"/>
        </property>
    </class>
</hibernate-mapping>